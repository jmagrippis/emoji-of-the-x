		"supabase": "^1.44.0",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vitest": "^0.29.7"
	},
	"type": "module",
	"engines": {
		"node": "16.x"
	},
	"volta": {
		"node": "16.19.1"
	}
