{
	"name": "emoji-of-the-x",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "npx supabase gen types typescript --local > src/lib/generated/database.types.ts && npm run format"
	},
	"devDependencies": {
		"@fontsource/alef": "^4.5.9",
		"@fontsource/noto-color-emoji": "^4.5.2",
		"@playwright/test": "^1.31.1",
		"@poppanator/sveltekit-svg": "^2.1.2",
		"@supabase/auth-helpers-sveltekit": "^0.9.0",
		"@supabase/supabase-js": "^2.11.0",
		"@sveltejs/adapter-vercel": "^2.1.1",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"happy-dom": "^8.9.0",
		"openai": "^3.2.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.2.3",
		"supabase": "^1.42.7",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"engines": {
		"node": "16.x"
	},
	"volta": {
		"node": "16.19.1"
	}
}
